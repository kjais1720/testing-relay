{
    "name": "@saastack/designations",
    "version": "0.1.58",
    "private": true,
    "scripts": {
        "start": "start-storybook -p 6006",
        "build": "gulp build",
        "release": "gulp release",
        "build-storybook": "build-storybook",
        "get-schema": "graphql get-schema --project app",
        "commit": "commit",
        "prepublishOnly": "exit 0",
        "update": "(git pull || true) && ncu --packageFile package.json -u '/^(?!graphql|use-debounce|@lingui|node-sass|@types/react-relay|@types/relay-runtime).*/' && npm i --no-audit"
    },
    "main": "index.js",
    "types": "index.d.ts",
