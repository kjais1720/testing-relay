{
  "name": "@saastack/designations",
  "version": "0.1.50",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 6006 --no-dll",
    "build": "gulp build",
    "release": "gulp release",
    "build-storybook": "build-storybook",
    "get-schema": "graphql get-schema --project app",
    "commit": "commit",
    "prepublishOnly": "exit 0",
    "update": "(git pull || true) && ncu --packageFile package.json -u '/^(?!graphql|@pmmmwh|@loadable/component|@lingui|react-scripts|node-sass).*/' && npm i --no-audit"
